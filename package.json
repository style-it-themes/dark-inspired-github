{
  "name": "dark-gitHub",
  "title": "Dark GitHub",
  "version": "2.0.0",
  "license": "MIT",
  "homepage": "https://github.com/style-it-themes/dark-github",
  "repository": "style-it-themes/dark-github",
  "bugs": {
    "url": "https://github.com/style-it-themes/dark-github/issues/new/choose"
  },
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "updates": "^11.4.0",
    "versions": "^8.4.3",
    "eclint": "^2.8.1"
  },
  "scripts": {
    "patch": "versions -p -C patch dark-github.user.styl",
    "minor": "versions -p -C minor dark-github.user.styl",
    "major": "versions -p -C major dark-github.user.styl",
    "lintfix": "eclint fix dark-github.user.styl",
    "update": "updates -cuU && npm install"
  }
}
